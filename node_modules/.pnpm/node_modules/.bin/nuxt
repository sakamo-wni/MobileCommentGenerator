#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sakamo/Desktop/sandbox/MobileCommentGenerator/node_modules/.pnpm/nuxt@3.17.5_@parcel+watcher@2.5.1_@types+node@20.19.1_db0@0.3.2_eslint@9.29.0_jiti@2.4._4da86f4472f6b573fc8ec9b4240b9d33/node_modules/nuxt/bin/node_modules:/Users/sakamo/Desktop/sandbox/MobileCommentGenerator/node_modules/.pnpm/nuxt@3.17.5_@parcel+watcher@2.5.1_@types+node@20.19.1_db0@0.3.2_eslint@9.29.0_jiti@2.4._4da86f4472f6b573fc8ec9b4240b9d33/node_modules/nuxt/node_modules:/Users/sakamo/Desktop/sandbox/MobileCommentGenerator/node_modules/.pnpm/nuxt@3.17.5_@parcel+watcher@2.5.1_@types+node@20.19.1_db0@0.3.2_eslint@9.29.0_jiti@2.4._4da86f4472f6b573fc8ec9b4240b9d33/node_modules:/Users/sakamo/Desktop/sandbox/MobileCommentGenerator/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/sakamo/Desktop/sandbox/MobileCommentGenerator/node_modules/.pnpm/nuxt@3.17.5_@parcel+watcher@2.5.1_@types+node@20.19.1_db0@0.3.2_eslint@9.29.0_jiti@2.4._4da86f4472f6b573fc8ec9b4240b9d33/node_modules/nuxt/bin/node_modules:/Users/sakamo/Desktop/sandbox/MobileCommentGenerator/node_modules/.pnpm/nuxt@3.17.5_@parcel+watcher@2.5.1_@types+node@20.19.1_db0@0.3.2_eslint@9.29.0_jiti@2.4._4da86f4472f6b573fc8ec9b4240b9d33/node_modules/nuxt/node_modules:/Users/sakamo/Desktop/sandbox/MobileCommentGenerator/node_modules/.pnpm/nuxt@3.17.5_@parcel+watcher@2.5.1_@types+node@20.19.1_db0@0.3.2_eslint@9.29.0_jiti@2.4._4da86f4472f6b573fc8ec9b4240b9d33/node_modules:/Users/sakamo/Desktop/sandbox/MobileCommentGenerator/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nuxt/bin/nuxt.mjs" "$@"
else
  exec node  "$basedir/../nuxt/bin/nuxt.mjs" "$@"
fi
