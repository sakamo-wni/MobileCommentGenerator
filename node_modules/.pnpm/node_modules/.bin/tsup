#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sakamo/Desktop/sandbox/MobileCommentGenerator/node_modules/.pnpm/tsup@8.5.0_jiti@2.4.2_postcss@8.5.6_typescript@5.8.3_yaml@2.8.0/node_modules/tsup/dist/node_modules:/Users/sakamo/Desktop/sandbox/MobileCommentGenerator/node_modules/.pnpm/tsup@8.5.0_jiti@2.4.2_postcss@8.5.6_typescript@5.8.3_yaml@2.8.0/node_modules/tsup/node_modules:/Users/sakamo/Desktop/sandbox/MobileCommentGenerator/node_modules/.pnpm/tsup@8.5.0_jiti@2.4.2_postcss@8.5.6_typescript@5.8.3_yaml@2.8.0/node_modules:/Users/sakamo/Desktop/sandbox/MobileCommentGenerator/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/sakamo/Desktop/sandbox/MobileCommentGenerator/node_modules/.pnpm/tsup@8.5.0_jiti@2.4.2_postcss@8.5.6_typescript@5.8.3_yaml@2.8.0/node_modules/tsup/dist/node_modules:/Users/sakamo/Desktop/sandbox/MobileCommentGenerator/node_modules/.pnpm/tsup@8.5.0_jiti@2.4.2_postcss@8.5.6_typescript@5.8.3_yaml@2.8.0/node_modules/tsup/node_modules:/Users/sakamo/Desktop/sandbox/MobileCommentGenerator/node_modules/.pnpm/tsup@8.5.0_jiti@2.4.2_postcss@8.5.6_typescript@5.8.3_yaml@2.8.0/node_modules:/Users/sakamo/Desktop/sandbox/MobileCommentGenerator/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tsup/dist/cli-default.js" "$@"
else
  exec node  "$basedir/../tsup/dist/cli-default.js" "$@"
fi
